{
  "LambdaFunctionConfigurations": [
    {
      "LambdaFunctionArn": "{{LambdaArn}}",
      "Events": ["s3:ObjectCreated:*"],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "prefix",
              "Value": "fwfidata/rloi/"
            }, {
              "Name": "suffix",
              "Value": ".XML"
            }
          ]
        }
      }
    }
  ]
}
